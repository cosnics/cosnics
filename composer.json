{
    "name": "cosnics/cosnics",
    "description": "Cosnics",
    "license": "MIT",
    "keywords": [
        "ELO",
        "LCMS",
        "Cosnics"
    ],
    "support": {
        "source": "https://github.com/cosnics/cosnics",
        "issues": "https://github.com/cosnics/cosnics/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-pdo": "*",

        "chamilo/pear-libraries": "*",

        "doctrine/cache": "1.12.*",
        "doctrine/dbal": "3.3.*",
        "doctrine/migrations": "3.4.*",
        "doctrine/orm": "2.11.*",
        "adodb/adodb-php": "5.*",

        "google/apiclient": "1.*",
        "guzzlehttp/guzzle": "7.*",
        "jms/serializer": "1.12.1",
        "monolog/monolog": "1.23.0",

        "symfony/cache": "5.4.*",
        "symfony/config": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dependency-injection": "5.4.*",
        "symfony/event-dispatcher": "5.4.*",
        "symfony/finder": "5.4.*",
        "symfony/form": "5.4.*",
        "symfony/http-foundation": "5.4.*",
        "symfony/http-kernel": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bridge": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*",

        "twig/twig": "2.*"
    },
    "require-dev": {
        "composer/composer": "*@dev",
        "fabpot/goutte": "4.*",
        "nelmio/alice": "3.*",
        "phpunit/phpunit": "7.2.6",
        "phpstan/phpstan": "1.5.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cosnics/pear-libraries"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chamilo\\": "src/Chamilo/"
        },
        "classmap": [
            "src/Chamilo/Libraries/Format/Form/Element/",
            "src/Chamilo/Configuration/Plugin"
        ],
        "exclude-from-classmap": [
            "src/Chamilo/Libraries/Format/Form/Element/AdvancedElementFinder"
        ]
    },
    "config": {
        "bin-dir": "bin/"
    },
    "scripts": {
        "pre-install-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer",
        "pre-update-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer"
    }
}
