{
    "name": "cosnics/cosnics",
    "description": "Cosnics",
    "license": "MIT",
    "keywords": [
        "ELO",
        "LCMS",
        "Cosnics"
    ],
    "support": {
        "source": "https://github.com/cosnics/cosnics",
        "issues": "https://github.com/cosnics/cosnics/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-pdo": "*",
        "chamilo/pear-libraries": "dev-refactor/database",
        "composer/composer": "2.3.*",
        "doctrine/annotations": "1.14.*",
        "doctrine/cache": "1.13.*",
        "doctrine/collections": "1.8.*",
        "doctrine/dbal": "3.6.*",
        "doctrine/orm": "2.11.*",
        "google/apiclient": "1.*",
        "guzzlehttp/guzzle": "7.*",
        "monolog/monolog": "2.*",
        "symfony/cache": "6.4.*",
        "symfony/config": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/event-dispatcher": "6.4.*",
        "symfony/finder": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/form": "6.4.*",
        "symfony/http-foundation": "6.4.*",
        "symfony/http-kernel": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/twig-bridge": "6.4.*",
        "symfony/uid": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/yaml": "6.4.*",
        "twig/twig": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "phpstan/phpstan": "1.5.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cosnics/pear-libraries"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chamilo\\": "src/Chamilo/"
        },
        "classmap": [
            "src/Chamilo/Libraries/Format/Form/Element/",
            "src/Chamilo/Configuration/Plugin"
        ],
        "exclude-from-classmap": [
            "src/Chamilo/Libraries/Format/Form/Element/AdvancedElementFinder"
        ]
    },
    "config": {
        "bin-dir": "bin/",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "pre-install-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer",
        "pre-update-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer"
    }
}
