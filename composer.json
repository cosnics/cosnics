{
	"name": "cosnics/cosnics",
	"description": "Cosnics",
	"license": "MIT",
	"keywords": [
		"ELO",
		"LCMS",
		"Cosnics"
	],
	"support": {
		"source": "https://github.com/cosnics/cosnics",
		"issues": "https://github.com/cosnics/cosnics/issues"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=8.2.0",
		"chamilo/pear-libraries": "dev-refactor/database",
		"doctrine/cache": "1.12.1",
		"doctrine/dbal": "2.13.8",
		"doctrine/migrations": "3.4.1",
		"doctrine/orm": "2.11.2",
		"google/apiclient": "1.*",
		"guzzlehttp/guzzle": "7.4.2",
		"jms/serializer": "3.17.1",
		"monolog/monolog": "1.23.0",
		"symfony/config": "^5.4",
		"symfony/console": "^5.4",
		"symfony/dependency-injection": "^5.4",
		"symfony/event-dispatcher": "^5.4",
		"symfony/finder": "^5.4",
		"symfony/form": "^5.4",
		"symfony/http-foundation": "^5.4",
		"symfony/http-kernel": "^5.4",
		"symfony/translation": "^5.4",
		"symfony/twig-bridge": "^5.4",
		"symfony/validator": "^5.4",
		"symfony/yaml": "^5.4",
		"symfony/security-csrf": "^5.4",
		"twig/twig": "^3.7",
		"assetic/framework": "3.0.0",
		"wikimedia/minify": "2.2.6",
		"ext-json": "*",
		"endroid/qr-code": "^3.5",
		"aws/aws-sdk-php": "*",
		"ext-dom": "*"
    },
	"require-dev": {
		"behat/behat": "*",
		"composer/composer": "*@dev",
		"fabpot/goutte": "^4",
		"nelmio/alice": "^3.0@RC",
		"phpunit/phpunit": "9.5.19",
		"phpstan/phpstan": "1.5.2"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/cosnics/pear-libraries"
		}
	],
	"autoload": {
		"psr-4": {
			"Chamilo\\": "src/Chamilo/"
		}
	},
	"config": {
		"bin-dir": "bin/",
		"allow-plugins": {
			"php-http/discovery": true
		}
	},
	"scripts": {
		"pre-install-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer",
		"pre-update-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer",
		"post-update-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processResources",
		"post-install-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processResources",
		"new-post-update-cmd": "@php console chamilo:process_resources",
		"new-post-install-cmd": "@php console chamilo:process_resources"
	}
}
