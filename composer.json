{
    "name" : "chamilo/lcms-connect",
    "description" : "Chamilo LCMS Connect",
    "license" : [ "GPL-3.0" ],
    "keywords" : [ "ELO", "LCMS" ],
    "support" : {
        "forum" : "http://lcms.chamilo.org/phpbbforum",
        "source" : "https://bitbucket.org/chamilo/lcms-connect/src",
        "issues" : "https://support.chamilo.org/projects/lcms-backlog"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "symfony/http-foundation" : "2.3.*",
        "doctrine/dbal" : "2.4.*",
        "doctrine/cache" : "1.4.*",
        "pear-pear.php.net/HTML_Common" : "*",
        "pear-pear.php.net/HTML_Menu" : "*",
        "pear-pear.php.net/HTML_Table" : "*",
        "pear-pear.php.net/HTML_QuickForm" : "*",
        "pear-pear.php.net/HTML_QuickForm_Controller" : "*",
        "pear-pear.php.net/HTML_QuickForm_advmultiselect" : "*",
        "pear-pear.php.net/PEAR" : "*",
        "pear-pear.php.net/XML_Serializer" : "*",
        "pear-pear.php.net/Pager" : "*",
        "pear-pear.php.net/Net_URL2" : "*",
        "pear-pear.php.net/Net_URL" : "*",
        "pear-pear.php.net/Net_Socket" : "*",
        "pear-pear.php.net/HTTP_Request" : "*",
        "pear-pear.php.net/HTTP_Request2" : "*",
        "pear-pear.php.net/MDB2" : "*",
        "pear-pear.php.net/MDB2_Driver_mysqli" : "*",
        "glenscott/soundcloud": "dev-psr-4",
        "google/apiclient": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "4.4.*",
        "behat/behat" : "*",
        "behat/mink" : "*",
        "behat/mink-extension" : "*",
        "behat/mink-goutte-driver" : "*",
        "fabpot/goutte" : "*",
        "composer/composer": "*@dev"
    },
    "repositories" : [
        {
            "type" : "pear",
            "url" : "http://pear.php.net"
        },
        {
            "type": "git",
            "url": "https://github.com/mptre/php-soundcloud"
        },
        {
            "type": "vcs",
            "url": "https://github.com/yakovlev-vladimir/FastFeed/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chamilo\\": "src/Chamilo/"
        }
    },
    "config" : {
        "bin-dir" : "bin/"
    },
    "extra" : {
        "install-path" : ""
    },
    "scripts":{
        "pre-install-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer",
        "pre-update-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processComposer",
        "post-update-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processResources",
        "post-install-cmd": "Chamilo\\Libraries\\Format\\Utilities\\BuildUtilities::processResources"
    }
}
