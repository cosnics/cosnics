{
    "name": "cosnics/libraries",
    "version": "1.0.0",
    "description": "Libraries",
    "require": {
        "php": ">=7.2.0",
        "pclzip/pclzip": "2.8.2",
        "phpmailer/phpmailer": "=v6.0.5",
        "kriswallsmith/assetic": "1.x",
        "voku/stringy": "6.*",
        "spipu/html2pdf": "=5.1.0",
        "phpoffice/phpspreadsheet": "1.8.0",
        "kigkonsult/icalcreator": "2.24",
        "simplepie/simplepie": "1.*",
        "ezyang/htmlpurifier": "4.9.2",
        "sentry/sentry": "^1.10",
        "google/recaptcha": "~1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.16.*"
    },
    "keywords": [
        "Libraries"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "extra": {
        "cosnics": {
            "name": "Libraries",
            "context": "Chamilo\\Libraries",
            "type": "Chamilo",
            "install": {
                "core": 1,
                "default": 0
            },
            "resources": [
                {
                    "output": "Chamilo/Libraries/Resources/Javascript/",
                    "input": "Resources/Javascript/"
                },
                {
                    "output": "Chamilo/Libraries/Format/Resources/Javascript/TblChange.js",
                    "input": "Format/Resources/Javascript/TblChange.js"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Plugin/",
                    "input": "Resources/Plugin/"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Images/",
                    "input": "Resources/Images/"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Javascript/cosnics.vendor.jquery.min.js",
                    "input": [
                        "Resources/Plugin/Jquery/jquery.min.js",
                        "Resources/Plugin/Jquery/jquery.browser.min.js",
                        "Resources/Plugin/Jquery/jquery-ui.min.js",
                        "Resources/Plugin/Jquery/jquery.timeout.interval.idle.min.js",
                        "Resources/Plugin/Jquery/jquery.mousewheel.min.js",
                        "Resources/Plugin/Jquery/jquery.scrollable.pack.js",
                        "Resources/Plugin/Jquery/jquery.xml2json.pack.js",
                        "Resources/Plugin/Jquery/jquery.json.min.js",
                        "Resources/Plugin/Jquery/jquery.query.min.js",
                        "Resources/Plugin/JqueryContextMenu/jquery.contextMenu.min.js"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Javascript/cosnics.vendor.bootstrap.min.js",
                    "input": [
                        "Resources/Plugin/Bootstrap/bootstrap.min.js",
                        "Resources/Plugin/Bootstrap/bootstrap-toggle.min.js",
                        "Resources/Plugin/Bootstrap/BootstrapConflictFixes.js"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Javascript/cosnics.vendor.angular.min.js",
                    "input": [
                        "Resources/Plugin/AngularJS/angular.min.js",
                        "Resources/Plugin/AngularJS/angular-sanitize.min.js",
                        "Resources/Plugin/AngularJS-UI-Bootstrap/ui-bootstrap.min.js"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Javascript/cosnics.vendor.min.js",
                    "input": [
                        "Resources/Plugin/Fancytree/dist/jquery.fancytree-all.min.js",
                        "Resources/Plugin/Dropzone/dropzone.min.js",
                        "Resources/Plugin/Highlight/highlight.pack.js",
                        "Resources/Plugin/PhpJs/phpjs.min.js"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Javascript/cosnics.common.min.js",
                    "input": [
                        "Resources/Javascript/Jquery/jquery.treeMenu.min.js",
                        "Resources/Javascript/Utilities.min.js",
                        "Resources/Javascript/Help.min.js",
                        "Resources/Javascript/Visit.min.js",
                        "Resources/Javascript/InitHighlight.min.js"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.vendor.bootstrap.min.css",
                    "input": [
                        "Resources/Plugin/Bootstrap/css/bootstrap.min.css",
                        "Resources/Plugin/BootstrapCheckbox/awesome-bootstrap-checkbox.min.css",
                        "Resources/Plugin/Bootstrap/css/bootstrap-toggle.min.css"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.vendor.jquery.min.css",
                    "input": [
                        "Resources/Plugin/Jquery/jquery-ui.min.css",
                        "Resources/Plugin/JqueryContextMenu/jquery.contextMenu.min.css"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.vendor.min.css",
                    "input": [
                        "Resources/Plugin/FontAwesome5/css/all.min.css",
                        "Resources/Plugin/ConnectIdents/ConnectIdents.min.css",
                        "Resources/Plugin/Dropzone/dropzone.min.css",
                        "Resources/Plugin/Highlight/github.min.css",
                        "Resources/Plugin/Fancytree/dist/skin-bootstrap/ui.fancytree.min.css"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Aqua.min.css",
                    "input": [
                        "Resources/Css/Aqua/General.min.css",
                        "Resources/Css/Aqua/Form.min.css",
                        "Resources/Css/Aqua/Table.min.css",
                        "Resources/Css/Aqua/Tabs.min.css",
                        "Resources/Css/Aqua/Tree.min.css",
                        "Resources/Css/Aqua/Bootstrap.min.css",
                        "Resources/Css/Aqua/Stylesheet.min.css"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Ruby.min.css",
                    "input": [
                        "Resources/Css/Aqua/General.min.css",
                        "Resources/Css/Aqua/Form.min.css",
                        "Resources/Css/Aqua/Table.min.css",
                        "Resources/Css/Aqua/Tabs.min.css",
                        "Resources/Css/Aqua/Tree.min.css",
                        "Resources/Css/Aqua/Bootstrap.min.css",
                        "Resources/Css/Aqua/Stylesheet.min.css",
                        "Resources/Css/Ruby/Stylesheet.min.css"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/ErrorPages.Aqua.min.css",
                    "input": "Resources/Css/Aqua/ErrorPages.min.css"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/ErrorPages.Ruby.min.css",
                    "input": "Resources/Css/Ruby/ErrorPages.min.css"
                }
            ],
            "additional": [
                "Chamilo\\Libraries\\Calendar",
                "Chamilo\\Libraries\\Rights"
            ],
            "dependencies": [
                {
                    "id": "Chamilo\\Configuration",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}
