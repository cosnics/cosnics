{
    "name": "cosnics/libraries-mail-mailer-platform",
    "version": "1.0.0",
    "description": "Libraries Mail Mailer Platform",
    "keywords": [
        "Libraries",
        "Mail",
        "Mailer",
        "Platform"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "extra": {
        "cosnics": {
            "name": "Default Mailer",
            "context": "Chamilo\\Libraries\\Mail\\Mailer\\Platform",
            "type": "Chamilo\\Libraries\\Mail\\Mailer",
            "resources": [
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Aqua.min.css",
                    "input": "Resources/Css/Aqua/Stylesheet.min.css"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Ruby.min.css",
                    "input": "Resources/Css/Aqua/Stylesheet.min.css"
                }
            ],
            "install": {
                "core": 1,
                "default": 0
            },
            "dependencies": [
                {
                    "id": "Chamilo\\Libraries",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}