{
    "name": "cosnics/libraries-calendar",
    "version": "1.0.0",
    "description": "Libraries Calendar",
    "keywords": [
        "Libraries",
        "Calendar"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "sabre/vobject": "4.*"
    },
    "extra": {
        "cosnics": {
            "name": "Calendar",
            "context": "Chamilo\\Libraries\\Calendar",
            "type": "Chamilo\\Libraries",
            "resources": [
                {
                    "output": "Chamilo/Libraries/Calendar/Resources/Javascript/",
                    "input": "Resources/Javascript/"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Aqua.min.css",
                    "input": "Resources/Css/Aqua/Stylesheet.min.css"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Ruby.min.css",
                    "input": [
                        "Resources/Css/Aqua/Stylesheet.min.css",
                        "Resources/Css/Ruby/Stylesheet.min.css"
                    ]
                }
            ],
            "install": {
                "core": 1,
                "default": 0
            },
            "dependencies": [
                {
                    "id": "Chamilo\\Libraries",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}