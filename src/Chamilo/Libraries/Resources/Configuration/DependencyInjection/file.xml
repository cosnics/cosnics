<?xml version="1.0" encoding="UTF-8" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service
            id="chamilo.libraries.file.path"
            class="Chamilo\Libraries\File\Path">
            <argument
                type="service"
                id="chamilo.libraries.architecture.classname_utilities" />
        </service>
        <service
            id="chamilo.libraries.file.configurable_path"
            class="Chamilo\Libraries\File\ConfigurablePath">
            <argument
                type="service"
                id="chamilo.configuration.service.configuration_service" />
        </service>
    </services>
</container>