<?xml version="1.0" encoding="UTF-8" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" autoconfigure="true" autowire="true" />
        <service
            id="chamilo.libraries.translation.translator_factory"
            class="Chamilo\Libraries\Translation\TranslatorFactory" >
            <argument
                    type="service"
                    id="chamilo.libraries.file.configurable_path_builder" />
        </service>
    </services>
</container>