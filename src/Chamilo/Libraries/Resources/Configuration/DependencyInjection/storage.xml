<?xml version="1.0" encoding="UTF-8" ?>
<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true" autoconfigure="true" public="true"/>
        <service
                id="Chamilo\Libraries\Storage\Cache\ConditionPartCache"/>

        <service
                id="Chamilo\Libraries\Storage\Cache\DataClassRepositoryCache"/>

        <service
                id="Chamilo\Libraries\Storage\DataClass\DataClassFactory"/>

        <service
                id="Chamilo\Libraries\Storage\DataManager\StorageAliasGenerator">
            <argument
                    type="service"
                    id="Chamilo\Libraries\Architecture\ClassnameUtilities"/>
        </service>

        <service id="Chamilo\Libraries\Storage\DataClass\PropertyMapper"/>

        <service id="Chamilo\Libraries\Storage\Service\DisplayOrderHandler"/>
        <service id="Chamilo\Libraries\Storage\Service\SearchQueryConditionGenerator"/>

        <service id="Chamilo\Libraries\Storage\DataManager\Repository\DisplayOrderRepository"/>
    </services>
</container>