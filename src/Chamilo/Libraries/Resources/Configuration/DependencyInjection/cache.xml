<?xml version="1.0" encoding="UTF-8" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service
            id="chamilo.libraries.cache.cache_director"
            class="Chamilo\Libraries\Cache\CacheDirector">
            <factory
                service="chamilo.libraries.cache.cache_director_builder"
                method="buildCacheDirector" />
        </service>

        <service
            id="chamilo.libraries.cache.cache_director_builder"
            class="Chamilo\Libraries\Cache\CacheDirectorBuilder">
            <argument
                type="service"
                id="service_container" />
        </service>
    </services>
</container>