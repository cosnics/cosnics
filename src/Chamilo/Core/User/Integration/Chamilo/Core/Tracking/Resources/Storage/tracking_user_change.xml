<?xml version="1.0" encoding="UTF-8"?>
<object name="tracking_user_change">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="user_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="reference_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="action"
                  type="text"
                  length="50"
                  notnull="1"
                  default="create"/>
        <property name="date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="idx_user_id">
            <property name="user_id"/>
        </index>
        <index name="idx_reference_id">
            <property name="reference_id"/>
        </index>
    </indexes>
</object>
