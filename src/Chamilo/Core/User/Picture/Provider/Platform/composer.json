{
    "name": "cosnics/core-user-picture-provider-platform",
    "version": "1.0.0",
    "description": "Core User Picture Provider Platform",
    "keywords": [
        "Core",
        "User",
        "Picture",
        "Provider",
        "Platform"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.4.0"
    },
    "extra": {
        "cosnics": {
            "name": "Platform User Picture Provider",
            "context": "Chamilo\\Core\\User\\Picture\\Provider\\Platform",
            "type": "Chamilo\\Core\\User\\Picture\\Provider",
            "resources": [
                {
                    "output": "Chamilo/Core/User/Picture/Provider/Platform/Resources/Images/",
                    "input": "Resources/Images/"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Aqua.min.css",
                    "input": "Resources/Css/Aqua/Stylesheet.min.css"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Ruby.min.css",
                    "input": "Resources/Css/Aqua/Stylesheet.min.css"
                }
            ],
            "install": {
                "core": 1,
                "default": 0
            },
            "dependencies": [
                {
                    "id": "Chamilo\\Core\\User",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}