<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true"
                  autoconfigure="true"
                  public="true"/>

        <service id="Chamilo\Core\User\Roles\Service\RoleService">
            <argument type="service"
                      id="Chamilo\Core\User\Roles\Storage\Repository\RoleRepository"/>
        </service>

        <service id="Chamilo\Core\User\Roles\Service\UserRoleService">
            <argument type="service"
                      id="Chamilo\Core\User\Roles\Service\RoleService"/>
            <argument type="service"
                      id="Chamilo\Core\User\Roles\Storage\Repository\UserRoleRepository"/>
        </service>

        <service id="Chamilo\Core\User\Roles\Service\Interfaces\UserRoleServiceInterface"
                 alias="Chamilo\Core\User\Roles\Service\UserRoleService"/>

    </services>
</container>