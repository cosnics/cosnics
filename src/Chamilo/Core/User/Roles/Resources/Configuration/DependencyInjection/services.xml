<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="chamilo.core.user.roles.service.role_service"
                 class="Chamilo\Core\User\Roles\Service\RoleService">
            <argument type="service" id="chamilo.core.user.roles.storage.repository.role_repository" />
        </service>

        <service id="chamilo.core.user.roles.service.user_role_service"
                 class="Chamilo\Core\User\Roles\Service\UserRoleService">
            <argument type="service" id="chamilo.core.user.roles.service.role_service" />
            <argument type="service" id="chamilo.core.user.roles.storage.repository.user_role_repository" />
        </service>
    </services>
</container>