        </service>

        <service id="Chamilo\Core\User\Service\Menu\LogoutItemRenderer">
            <tag name="Chamilo\Core\Menu\Renderer\ItemRenderer"/>
        </service>

        <service id="Chamilo\Core\User\Service\Menu\WidgetItemRenderer">
            <tag name="Chamilo\Core\Menu\Renderer\ItemRenderer"/>
        </service>
    </services>
</container>