<?xml version="1.0" encoding="UTF-8"?>
<object name="metadata_element">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="schema_id"
                  type="integer"
                  notnull="1"/>
        <property name="name"
                  type="string"
                  length="255"
                  notnull="1"/>
        <property name="display_name"
                  type="string"
                  length="255"
                  notnull="1"/>
        <property name="fixed"
                  type="integer"
                  notnull="1"/>
        <property name="display_order"
                  type="integer"
                  notnull="1"/>
        <property name="value_type"
                  type="integer"
                  notnull="1"/>
        <property name="value_limit"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="unq_element_name"
               type="unique">
            <property name="name"/>
            <property name="schema_id"/>
        </index>
        <index name="idx_schema_id">
            <property name="schema_id"/>
        </index>
    </indexes>
</object>