<?xml version="1.0" encoding="UTF-8"?>
<block name="external">
	<settings>
		<category name="object">
			<setting name="use_object" field="select" default="0">
				<options type="dynamic" source="get_external_objects" />
			</setting>
			<setting name="scrolling" field="select" default="no">
				<options type="static">
					<option name="No" value="no" />
					<option name="Yes" value="yes" />
					<option name="Auto" value="auto" />
				</options>
			</setting>
			<setting name="height" field="text" default="300" />
		</category>
	</settings>
</block>
