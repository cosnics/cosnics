<?xml version="1.0" encoding="UTF-8" ?>
<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" autowire="true" />
        <service
                id="chamilo.core.group.service.group_service"
                class="Chamilo\Core\Group\Service\GroupService">
            <argument type="service" id="chamilo.core.group.storage.repository.group_repository" />
        </service>

        <service alias="chamilo.core.group.service.group_service"
                 id="Chamilo\Core\Group\Service\GroupService" />

    </services>
</container>