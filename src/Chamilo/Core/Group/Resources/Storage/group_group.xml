<?xml version="1.0" encoding="UTF-8"?>
<object name="group_group">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="name"
                  type="text"
                  length="255"
                  notnull="1"
                  default=""/>
        <property name="description"
                  type="text"/>
        <property name="parent_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="left_value"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="right_value"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="sort"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="code"
                  type="text"
                  length="50"
                  fixed="1"
                  notnull="1"
                  default=""/>
        <property name="disk_quota"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="209715200"/>
        <property name="database_quota"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="10000"/>
    </properties>
    <indexes>
        <index name="idx_parent_id">
            <property name="parent_id"/>
        </index>
        <index name="idx_code">
            <property name="code"/>
        </index>
        <index name="idx_sort">
            <property name="sort"/>
        </index>
        <index name="idx_position">
            <property name="parent_id"/>
            <property name="left_value"/>
            <property name="right_value"/>
        </index>
        <index name="idx_left_value">
            <property name="left_value"/>
        </index>
        <index name="idx_right_value">
            <property name="right_value"/>
        </index>
    </indexes>
</object>