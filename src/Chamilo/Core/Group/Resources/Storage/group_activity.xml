<?xml version="1.0" encoding="UTF-8"?>
<object name="group_activity">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="user_id"
                  type="string"
                  length="36"/>
        <property name="reference_id"
                  type="string"
                  length="36"
                  notnull="1"/>
        <property name="target_user_id"
                  type="string"
                  length="36"/>
        <property name="action"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="idx_user_id">
            <property name="user_id"/>
        </index>
        <index name="idx_reference_id">
            <property name="reference_id"/>
        </index>
        <index name="idx_target_user_id">
            <property name="target_user_id"/>
        </index>
    </indexes>
</object>