<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true"
                  autoconfigure="true"
                  public="true"/>
        <service id="Chamilo\Core\Repository\Console\Command\ContentObjectTemplateSynchronizerCommand">
            <argument type="service"
                      id="Chamilo\Core\Repository\Service\ContentObjectTemplate\ContentObjectTemplateSynchronizer"/>
            <argument type="service"
                      id="Symfony\Component\Translation\Translator"/>
            <tag name="chamilo.libraries.console.command"/>
        </service>

        <service id="Chamilo\Core\Repository\Console\Command\ResourceFixerCommand">
            <argument type="service"
                      id="Chamilo\Core\Repository\Service\ResourceFixer\ResourceFixerDirector"/>
            <argument type="service"
                      id="Symfony\Component\Translation\Translator"/>
            <tag name="chamilo.libraries.console.command"/>
        </service>
    </services>
</container>