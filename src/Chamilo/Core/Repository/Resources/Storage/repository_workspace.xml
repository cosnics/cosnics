<?xml version="1.0" encoding="UTF-8"?>
<object name="repository_workspace">
    <properties>
        <property name="id"
                  type="integer"
                  notnull="1"
                  autoincrement="true"/>
        <property name="name"
                  type="string"
                  length="100"
                  notnull="1"/>
        <property name="description"
                  type="text"/>
        <property name="creator_id"
                  type="integer"
                  notnull="1"
                  default="0"/>
        <property name="creation_date"
                  type="integer"
                  notnull="1"
                  default="0"/>
    </properties>
    <indexes>
        <index name="unq_name"
               type="unique">
            <property name="name"/>
            <property name="creator_id"/>
        </index>
    </indexes>
</object>