<?xml version="1.0" encoding="UTF-8"?>
<object name="workspace">
    <properties>
        <property name="id"
                  type="integer"
                  notnull="1"
                  autoincrement="true"/>
        <property name="name"
                  type="text"
                  length="100"
                  notnull="1"
                  default=""/>
        <property name="description"
                  type="text"/>
        <property name="creator_id"
                  type="integer"
                  notnull="1"
                  default="0"/>
        <property name="creation_date"
                  type="integer"
                  notnull="1"
                  default="0"/>
    </properties>
    <index name="name"
           type="unique">
        <indexproperty name="name"/>
        <indexproperty name="creator_id"/>
    </index>
</object>