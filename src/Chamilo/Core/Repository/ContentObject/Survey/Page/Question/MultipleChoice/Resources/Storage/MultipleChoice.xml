<?xml version="1.0" encoding="UTF-8"?>
<object name="multiple_choice">
    <properties>
        <property
            name="id"
            type="integer"
            unsigned="1"
            notnull="1" />
        <property
            name="answer_type"
            type="integer"
            unsigned="1"
            notnull="1"
            length="1" />
        <property
            name="display_type"
            type="text"
            length="20"
            fixed="true"
            notnull="1" />
        <property
            name="question"
            type="text"
            notnull="1" />
        <property
            name="instruction"
            type="text" />
    </properties>
    <index
        name="id"
        type="primary">
        <indexproperty name="id" />
    </index>
    <index name="answer_type">
        <indexproperty name="answer_type" />
    </index>
    <index name="display_type">
        <indexproperty name="display_type" />
    </index>
</object>