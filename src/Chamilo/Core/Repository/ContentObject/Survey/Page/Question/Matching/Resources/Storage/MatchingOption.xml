<?xml version="1.0" encoding="UTF-8"?>
<object name="matching_option">
    <properties>
        <property
            name="id"
            type="integer"
            unsigned="1"
            notnull="1"
            autoincrement="true" />
        <property
            name="option_value"
            type="text"
            length="150"
            fixed="true"
            notnull="1" />
        <property
            name="question_id"
            type="integer"
            unsigned="1"
            notnull="1" />
        <property
            name="display_order"
            type="integer"
            unsigned="1"
            notnull="1"
            default="0" />
    </properties>
</object>