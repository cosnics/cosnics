<?xml version="1.0" encoding="UTF-8"?>
<object name="repository_assessment_open_question">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="question_type"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="1"/>
        <property name="feedback"
                  type="text"/>
        <property name="hint"
                  type="text"/>
    </properties>
    <indexes>
        <index name="prm_id"
               type="primary">
            <property name="id"/>
        </index>
        <index name="idx_question_type">
            <property name="question_type"/>
        </index>
    </indexes>
</object>