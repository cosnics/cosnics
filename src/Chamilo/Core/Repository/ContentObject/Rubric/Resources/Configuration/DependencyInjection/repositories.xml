<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="Chamilo\Core\Repository\ContentObject\Rubric\Storage\Repository\RubricDataRepository">
            <factory service="doctrine.orm.entity_manager" method="getRepository" />
            <argument>Chamilo\Core\Repository\ContentObject\Rubric\Storage\Entity\RubricData</argument>
        </service>

        <service id="Chamilo\Core\Repository\ContentObject\Rubric\Storage\Repository\TreeNodeRepository">
            <factory service="doctrine.orm.entity_manager" method="getRepository" />
            <argument>Chamilo\Core\Repository\ContentObject\Rubric\Storage\Entity\TreeNode</argument>
        </service>

    </services>
</container>
