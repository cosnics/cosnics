<?xml version="1.0" encoding="UTF-8"?>
<object name="repository_webpage">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="storage_path"
                  type="text"
                  length="255"
                  notnull="1"/>
        <property name="path"
                  type="text"
                  length="255"
                  notnull="1"/>
        <property name="filename"
                  type="text"
                  length="255"
                  notnull="1"/>
        <property name="filesize"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="hash"
                  type="text"
                  length="50"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="prm_id"
               type="primary">
            <property name="id"/>
        </index>
        <index name="idx_hash">
            <property name="hash"/>
        </index>
    </indexes>
</object>
