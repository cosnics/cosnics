<?xml version="1.0" encoding="UTF-8" ?>
<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service
                id="chamilo.core.repository.content_object.learning_path.service.learning_path_tree_builder"
                class="Chamilo\Core\Repository\ContentObject\LearningPath\Service\LearningPathTreeBuilder">
            <argument
                    type="service"
                    id="chamilo.core.repository.content_object.learning_path.storage.repository.learning_path_child_repository" />
            <argument type="service"
                      id="chamilo.core.repository.workspace.repository.content_object_repository" />
        </service>

        <service
                id="chamilo.core.repository.content_object.learning_path.service.learning_path_child_service"
                class="Chamilo\Core\Repository\ContentObject\LearningPath\Service\LearningPathChildService">
            <argument
                    type="service"
                    id="chamilo.core.repository.content_object.learning_path.storage.repository.learning_path_child_repository" />
        </service>

        <service
                id="chamilo.core.repository.content_object.learning_path.integration.chamilo.core.repository.publication.service.learning_path_publication_service"
                class="Chamilo\Core\Repository\ContentObject\LearningPath\Integration\Chamilo\Core\Repository\Publication\Service\LearningPathPublicationService">
            <argument
                    type="service"
                    id="chamilo.core.repository.content_object.learning_path.service.learning_path_child_service" />
            <argument
                    type="service"
                    id="chamilo.core.repository.content_object.learning_path.service.learning_path_tree_builder" />
            <argument type="service"
                      id="chamilo.core.repository.workspace.repository.content_object_repository" />
        </service>

    </services>
</container>