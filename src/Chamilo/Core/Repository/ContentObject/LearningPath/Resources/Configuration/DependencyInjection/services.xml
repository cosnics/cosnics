<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true"
                  autoconfigure="true"
                  public="true"/>

        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Service\TreeBuilder"/>
        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Service\TreeNodeDataService"/>
        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Service\LearningPathService"/>
        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Service\TreeNodeCopier"/>
        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Service\AutomaticNumberingService"/>
        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Integration\Chamilo\Core\Repository\Publication\Service\PublicationService"/>
        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Service\MigrationFixer"/>
        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Service\ActivityService"/>

        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Console\Command\MigrationCommand">
            <tag name="Chamilo\Libraries\Console\Command"/>
        </service>

        <service id="Chamilo\Core\Repository\ContentObject\LearningPath\Console\Command\MigrationFixerCommand">
            <tag name="Chamilo\Libraries\Console\Command"/>
        </service>

    </services>
</container>