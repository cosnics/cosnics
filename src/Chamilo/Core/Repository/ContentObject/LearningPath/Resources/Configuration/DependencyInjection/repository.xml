<?xml version="1.0" encoding="UTF-8" ?>
<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" autowire="true" />
        <service
                id="chamilo.core.repository.content_object.learning_path.storage.repository.tree_node_data_repository"
                class="Chamilo\Core\Repository\ContentObject\LearningPath\Storage\Repository\TreeNodeDataRepository">
            <argument
                    type="service"
                    id="chamilo.libraries.storage.data_manager.doctrine.data_class_repository" />
        </service>
        <service
                id="chamilo.core.repository.content_object.learning_path.storage.repository.learning_path_step_context_repository"
                class="Chamilo\Core\Repository\ContentObject\LearningPath\Storage\Repository\LearningPathStepContextRepository">
            <argument
                    type="service"
                    id="chamilo.libraries.storage.data_manager.doctrine.data_class_repository" />
        </service>

        <service
                alias="chamilo.core.repository.content_object.learning_path.storage.repository.learning_path_step_context_repository"
                id="Chamilo\Core\Repository\ContentObject\LearningPath\Storage\Repository\LearningPathStepContextRepository">
        </service>
    </services>
</container>
