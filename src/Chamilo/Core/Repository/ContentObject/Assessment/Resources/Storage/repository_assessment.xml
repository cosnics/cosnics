<?xml version="1.0" encoding="UTF-8"?>
<object name="repository_assessment">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="max_attempts"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="questions_per_page"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="max_time"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="random_questions"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
    </properties>
    <indexes>
        <index name="prm_id"
               type="primary">
            <property name="id"/>
        </index>
    </indexes>
</object>
