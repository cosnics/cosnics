<?xml version="1.0" encoding="UTF-8"?>
<object name="repository_task">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="category"
                  type="string"
                  length="50"/>
        <property name="priority"
                  type="integer"/>
        <property name="start_date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="due_date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="until"
                  type="integer"
                  unsigned="1"/>
        <property name="frequency"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="frequency_count"
                  type="integer"
                  unsigned="1"/>
        <property name="frequency_interval"
                  type="integer"
                  unsigned="1"/>
        <property name="byday"
                  type="string"
                  length="50"/>
        <property name="bymonthday"
                  type="string"
                  length="100"/>
        <property name="bymonth"
                  type="string"
                  length="50"/>
    </properties>
    <indexes>
        <index name="prm_id"
               type="primary">
            <property name="id"/>
        </index>
    </indexes>
</object>