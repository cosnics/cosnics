{
    "name": "cosnics/core-repository-implementation-google-docs",
    "version": "1.0.0",
    "description": "Core Repository Implementation GoogleDocs",
    "keywords": [
        "Core",
        "Repository",
        "Implementation",
        "GoogleDocs"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "zend/gdata": "1.*",
        "google/apiclient": "1.1.*"
    },
    "extra": {
        "cosnics": {
            "name": "Google Drive (Docs)",
            "context": "Chamilo\\Core\\Repository\\Implementation\\GoogleDocs",
            "type": "Chamilo\\Core\\Repository\\Implementation",
            "dependencies": [
                {
                    "id": "Chamilo\\Core\\Repository",
                    "version": ">=1.0.0"
                }
            ],
            "css": [
                {
                    "type": "common",
                    "themes": [
                        {
                            "type": "Aqua",
                            "files": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        },
                        {
                            "type": "Ruby",
                            "files": [
                                "Resources/Css/Aqua/Stylesheet.min.css",
                                "Resources/Css/Ruby/Stylesheet.min.css"
                            ]
                        }
                    ]
                }
            ],
            "install": {
                "default": 0,
                "core": 0
            }
        }
    }
