<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0" packagerversion="1.7.2">
	<name>PBAPI</name>
	<summary>Photobucket API Client</summary>
	<description>Photobucket API client for PHP5, with Fluent interface
	</description>
	<maintainers>
		<maintainer>
			<user>jhart</user>
			<name>Justin Hart</name>
			<email>jhart@photobucket.com</email>
			<role>lead</role>
		</maintainer>
	</maintainers>
	<release>
		<version>0.2.3</version>
		<date>2009-03-09</date>
		<license>The MIT License</license>
		<state>beta</state>
		<notes>small update
		</notes>
		<deps>
			<dep type="php" rel="ge" version="5.1.0" />
			<dep type="pkg" rel="ge" version="0.1" optional="no">OAuth</dep>
			<dep type="pkg" rel="ge" version="0.1" optional="yes">XML_Serializer</dep>
			<dep type="ext" rel="has" optional="yes">SimpleXML</dep>
			<dep type="ext" rel="has" optional="yes">json</dep>
			<dep type="ext" rel="has" optional="yes">syck</dep>
			<dep type="ext" rel="has" optional="yes">curl</dep>
		</deps>
		<filelist>
			<file role="php" name="PBAPI.php" />
			<file role="php" name="PBAPI/data/api-defs.yml" />
			<file role="php" name="PBAPI/data/methods.dat" />
			<file role="php" name="PBAPI/data/yml2phpserialize.php" />
			<file role="php" name="PBAPI/Exception/Response.php" />
			<file role="php" name="PBAPI/Methods/album.php" />
			<file role="php" name="PBAPI/Methods/base.php" />
			<file role="php" name="PBAPI/Methods/media.php" />
			<file role="php" name="PBAPI/Methods/user.php" />
			<file role="php" name="PBAPI/Methods/group.php" />
			<file role="php" name="PBAPI/Request/curl.php" />
			<file role="php" name="PBAPI/Request/fopenurl.php" />
			<file role="php" name="PBAPI/Request/fsockopen.php" />
			<file role="php" name="PBAPI/Response/json.php" />
			<file role="php" name="PBAPI/Response/phpserialize.php" />
			<file role="php" name="PBAPI/Response/simplexml.php" />
			<file role="php" name="PBAPI/Response/simplexmlarray.php" />
			<file role="php" name="PBAPI/Response/xmlserializer.php" />
			<file role="php" name="PBAPI/Exception.php" />
			<file role="php" name="PBAPI/Methods.php" />
			<file role="php" name="PBAPI/Request.php" />
			<file role="php" name="PBAPI/Response.php" />
		</filelist>
	</release>
</package>
