<?xml version="1.0" encoding="UTF-8"?>
<package context="Chamilo\Core\Repository\Implementation\Soundcloud">
	<settings>
		<category name="credentials">
			<setting name="key" field="text" default="">
				<validations>
					<validation rule="required" message="ThisFieldIsRequired" />
				</validations>
			</setting>
			<setting name="secret" field="text" default="">
				<validations>
					<validation rule="required" message="ThisFieldIsRequired" />
				</validations>
			</setting>
			<setting name="oauth_token" field="text" default="" user_setting="1" />
			<setting name="oauth_token_secret" field="text" default="" user_setting="1" />
		</category>
	</settings>
</package>
