<?xml version="1.0" encoding="UTF-8"?>
<object name="tracking_repository_activity">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="type"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="user_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="content_object_id"
                  type="integer"
                  unsigned="1"/>
        <property name="content"
                  type="text"/>
    </properties>
    <indexes>
        <index name="idx_content_object_id">
            <property name="content_object_id"/>
        </index>
        <index name="idx_user_id">
            <property name="user_id"/>
        </index>
    </indexes>
</object>