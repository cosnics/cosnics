{
    "name": "cosnics/core-repository",
    "version": "1.0.0",
    "description": "Core Repository",
    "keywords": [
        "Core",
        "Repository"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "phpspec/php-diff": "=v1.0.2"
    },
    "extra": {
        "cosnics": {
            "name": "Repository",
            "context": "Chamilo\\Core\\Repository",
            "type": "Chamilo\\Core",
            "resources": [
                {
                    "output": "Chamilo/Core/Repository/Workspace/Rights/Resources/Javascript/",
                    "input": "Workspace/Rights/Resources/Javascript/"
                },
                {
                    "output": "Chamilo/Core/Repository/Viewer/Resources/Javascript/",
                    "input": "Viewer/Resources/Javascript/"
                },
                {
                    "output": "Chamilo/Core/Repository/Publication/Resources/Javascript/",
                    "input": "Publication/Resources/Javascript/"
                },
                {
                    "output": "Chamilo/Core/Repository/Resources/Javascript/",
                    "input": "Resources/Javascript/"
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Aqua.min.css",
                    "input": [
                        "Feedback/Resources/Css/Aqua/Stylesheet.min.css",
                        "Resources/Css/Aqua/Stylesheet.min.css"
                    ]
                },
                {
                    "output": "Chamilo/Libraries/Resources/Css/cosnics.common.Ruby.min.css",
                    "input": [
                        "Feedback/Resources/Css/Aqua/Stylesheet.min.css",
                        "Resources/Css/Aqua/Stylesheet.min.css"
                    ]
                }
            ],
            "install": {
                "core": 1,
                "default": 0
            },
            "dependencies": [
                {
                    "id": "Chamilo\\Configuration",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}