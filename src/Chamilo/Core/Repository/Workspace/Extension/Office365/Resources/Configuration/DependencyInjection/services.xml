<?xml version="1.0" encoding="UTF-8" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true" public="true" />

        <prototype namespace="Chamilo\Core\Repository\Workspace\Extension\Office365\" resource="../../../{Service,Storage/Repository}/*"/>

        <service id="Chamilo\Core\Repository\Workspace\Extension\Office365\Service\Office365WorkspaceExtension">
            <tag name="chamilo.core.repository.workspace.extension" />
        </service>

    </services>
</container>