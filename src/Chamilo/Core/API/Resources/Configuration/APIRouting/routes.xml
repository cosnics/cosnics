<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="api_token" path="/api/token" controller="Chamilo\Core\API\Component\TokenComponent"/>
    <route id="api_doc" path="/api/doc" controller="Chamilo\Core\API\Component\DocComponent"/>
    <route id="api_test" path="/api/test/{id}" controller="Chamilo\Core\API\Component\TestComponent"/>
</routes>