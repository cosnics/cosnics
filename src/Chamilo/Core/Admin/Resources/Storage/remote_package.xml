            autoincrement="true" />
        <property
            name="code"
            type="text"
            length="255" />
        <property
            name="context"
            type="text"
            length="255" />
        <property
            name="name"
            type="text"
            length="255" />
        <property
            name="section"
            type="text"
            length="255" />
        <property
            name="category"
            type="text"
            length="255" />
        <property
            name="authors"
            type="text" />
        <property
            name="version"
            type="text"
            length="255" />
        <property
            name="cycle"
            type="text" />
        <property
            name="filename"
            type="text"
            length="255" />
        <property
            name="size"
            type="integer"
            notnull="0"
            default="0" />
        <property
            name="md5"
            type="text"
            length="32" />
        <property
            name="sha1"
            type="text"
            length="40" />
        <property
            name="sha256"
            type="text"
            length="64" />
