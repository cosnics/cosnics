<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true"
                  autoconfigure="true"
                  public="true"/>

        <service id="Chamilo\Core\Rights\Structure\Console\Command\SynchronizeStructureLocationsCommand">
            <argument type="service"
                      id="Chamilo\Core\Rights\Structure\Service\StructureLocationConfiguration\Synchronizer"/>
            <tag name="Chamilo\Libraries\Console\Command"/>
        </service>

    </services>
</container>