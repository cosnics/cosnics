{
    "name": "cosnics/core-menu",
    "version": "1.0.0",
    "description": "Core Menu",
    "keywords": [
        "Core",
        "Menu"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "extra": {
        "cosnics": {
            "name": "Menu",
            "context": "Chamilo\\Core\\Menu",
            "type": "Chamilo\\Core",
            "css": [
                {
                    "output": "cosnics.common.min.css",
                    "themes": [
                        {
                            "output": "cosnics.common.Aqua.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        },
                        {
                            "output": "cosnics.common.Ruby.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css",
                                "Resources/Css/Ruby/Stylesheet.min.css"
                            ]
                        }
                    ]
                }
            ],
            "install": {
                "core": 1,
                "default": 0
            },
            "dependencies": [
                {
                    "id": "Chamilo\\Configuration",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}