<?xml version="1.0" encoding="UTF-8"?>
<object name="menu_item">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="parent"
                  type="integer"
                  unsigned="1"
                  default="0"/>
        <property name="sort"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="type"
                  type="text"
                  length="255"
                  unsigned="1"
                  notnull="0"/>
        <property name="icon_class"
                  type="text"
                  length="255"
                  unsigned="1"
                  notnull="0"/>
        <property name="hidden"
                  type="integer"
                  length="1"
                  unsigned="1"
                  default="0"/>
        <property name="display"
                  type="integer"
                  length="1"
                  unsigned="1"
                  default="1"/>
        <property name="configuration"
                  type="text"
                  unsigned="1"
                  notnull="0"/>
    </properties>
    <indexes>
        <index name="idx_parent">
            <property name="parent"/>
        </index>
        <index name="idx_sort">
            <property name="sort"/>
        </index>
    </indexes>
</object>
