<?xml version="1.0" encoding="UTF-8"?>
<object name="configuration_registration">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="context"
                  type="text"
                  length="255"/>
        <property name="type"
                  type="text"
                  length="255"/>
        <property name="category"
                  type="text"
                  length="50"/>
        <property name="name"
                  type="text"
                  length="50"/>
        <property name="status"
                  type="integer"
                  length="1"
                  unsigned="1"
                  notnull="1"/>
        <property name="version"
                  type="text"
                  length="50"/>
        <property name="priority"
                  type="integer"
                  length="3"
                  unsigned="1"
                  notnull="1"
                  default="1"/>
    </properties>
    <indexes>
        <index name="idx_type">
            <property name="type"/>
        </index>
        <index name="idx_status">
            <property name="status"/>
        </index>
        <index name="unq_context"
               type="unique">
            <property name="context"/>
        </index>
    </indexes>
</object>