<?xml version="1.0" encoding="UTF-8"?>
<object name="configuration_form_value">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="user_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="time"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="dynamic_form_element_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="value"
                  type="text"
                  length="500"
                  notnull="1"
                  default=""/>
    </properties>
    <indexes>
        <index name="idx_user_id">
            <property name="user_id"/>
        </index>
        <index name="idx_dynamic_form_element_id">
            <property name="dynamic_form_element_id"/>
        </index>
    </indexes>
</object>
