<?xml version="1.0" encoding="UTF-8"?>
<object name="language">
    <properties>
        <property
            name="id"
            type="integer"
            unsigned="1"
            notnull="1"
            autoincrement="true" />
        <property
            name="original_name"
            type="text"
            length="255" />
        <property
            name="english_name"
            type="text"
            length="255" />
        <property
            name="family"
            type="text"
            length="255" />
        <property
            name="isocode"
            type="text"
            length="10" />
        <property
            name="available"
            type="boolean"
            default="1" />
    </properties>
    <index name="english_name">
        <indexproperty name="english_name" />
    </index>
    <index name="isocode">
        <indexproperty name="isocode" />
    </index>
</object>
