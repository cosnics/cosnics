<?xml version="1.0" encoding="UTF-8"?>
<object name="configuration_form_element">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="name"
                  type="string"
                  length="100"
                  notnull="1"
                  default=""/>
        <property name="type"
                  type="boolean"
                  notnull="1"/>
        <property name="required"
                  type="boolean"
                  notnull="1"/>
        <property name="dynamic_form_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="display_order"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="idx_name">
            <property name="name"/>
        </index>
        <index name="idx_type">
            <property name="type"/>
        </index>
        <index name="idx_dynamic_form_id">
            <property name="dynamic_form_id"/>
        </index>
    </indexes>
</object>