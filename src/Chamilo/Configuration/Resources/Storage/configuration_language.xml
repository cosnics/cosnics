<?xml version="1.0" encoding="UTF-8"?>
<object name="configuration_language">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="original_name"
                  type="string"
                  length="255"/>
        <property name="english_name"
                  type="string"
                  length="255"/>
        <property name="family"
                  type="string"
                  length="255"/>
        <property name="isocode"
                  type="string"
                  length="10"/>
        <property name="available"
                  type="boolean"
                  default="1"/>
    </properties>
    <indexes>
        <index name="idx_english_name">
            <property name="english_name"/>
        </index>
        <index name="idx_isocode">
            <property name="isocode"/>
        </index>
    </indexes>
</object>