<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	<services>
		<service id="chamilo.application.portfolio.service.rights_service"
			class="Chamilo\Application\Portfolio\Service\RightsService">
			<argument type="service"
				id="chamilo.application.portfolio.storage.repository.rights_repository" />
			<argument type="service" id="chamilo.core.user.service.user_service" />
			<argument type="service" id="symfony.component.translation.translator" />
			<argument type="service"
				id="chamilo.libraries.platform.session.session_utilities" />
			<argument type="service"
				id="Chamilo\Core\Repository\Workspace\Service\WorkspaceService" />
		</service>
		<service
			id="chamilo.application.portfolio.storage.repository.rights_repository"
			class="Chamilo\Application\Portfolio\Storage\Repository\RightsRepository">
			<argument type="service"
				id="chamilo.libraries.storage.data_manager.doctrine.data_class_repository" />
		</service>

		<service id="chamilo.application.portfolio.service.feedback_service"
			class="Chamilo\Application\Portfolio\Service\FeedbackService">
			<argument type="service"
				id="chamilo.application.portfolio.storage.repository.feedback_repository" />
			<argument type="service"
				id="chamilo.application.portfolio.service.rights_service" />
		</service>

		<service
			id="chamilo.application.portfolio.storage.repository.feedback_repository"
			class="Chamilo\Application\Portfolio\Storage\Repository\FeedbackRepository">
			<argument type="service"
				id="chamilo.libraries.storage.data_manager.doctrine.data_class_repository" />
		</service>

		<service id="chamilo.application.portfolio.service.notification_service"
			class="Chamilo\Application\Portfolio\Service\NotificationService">
			<argument type="service"
				id="chamilo.application.portfolio.storage.repository.notification_repository" />
		</service>

		<service
			id="chamilo.application.portfolio.storage.repository.notification_repository"
			class="Chamilo\Application\Portfolio\Storage\Repository\NotificationRepository">
			<argument type="service"
				id="chamilo.libraries.storage.data_manager.doctrine.data_class_repository" />
		</service>

		<service id="chamilo.application.portfolio.service.publication_service"
			class="Chamilo\Application\Portfolio\Service\PublicationService">
			<argument type="service"
				id="chamilo.application.portfolio.storage.repository.publication_repository" />
			<argument type="service"
				id="chamilo.application.portfolio.service.rights_service" />
			<argument type="service" id="symfony.component.translation.translator" />
		</service>

		<service
			id="chamilo.application.portfolio.storage.repository.publication_repository"
			class="Chamilo\Application\Portfolio\Storage\Repository\PublicationRepository">
			<argument type="service"
				id="chamilo.libraries.storage.data_manager.doctrine.data_class_repository" />
		</service>
	</services>
</container>