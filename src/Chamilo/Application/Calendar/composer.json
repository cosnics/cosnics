{
    "name": "cosnics/application-calendar",
    "version": "1.0.0",
    "description": "Application Calendar",
    "keywords": [
        "Application",
        "Calendar"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "extra": {
        "cosnics": {
            "name": "Personal Calendar",
            "context": "Chamilo\\Application\\Calendar",
            "type": "Chamilo\\Application",
            "category": "social",
            "resources": [
                {
                    "output": "Resources/Css/cosnics.common.min.css",
                    "themes": [
                        {
                            "output": "Resources/Css/cosnics.common.Aqua.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        },
                        {
                            "output": "Resources/Css/cosnics.common.Ruby.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        }
                    ]
                }
            ],
            "install": {
                "default": 1,
                "core": 0
            },
            "additional": [
                "Chamilo\\Application\\Calendar\\Extension\\Personal"
            ],
            "dependencies": [
                {
                    "id": "Chamilo\\Configuration",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}