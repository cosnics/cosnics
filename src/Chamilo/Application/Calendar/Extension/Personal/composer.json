{
    "name": "cosnics/application-calendar-extension-personal",
    "version": "1.0.0",
    "description": "Application Calendar Extension Personal",
    "keywords": [
        "Application",
        "Calendar",
        "Extension",
        "Personal"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
