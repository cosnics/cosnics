{
    "name": "cosnics/application-weblcms-admin-extension-platform",
    "version": "1.0.0",
    "description": "Application Weblcms Admin Extension Platform",
    "keywords": [
        "Application",
        "Weblcms",
        "Admin",
        "Extension",
        "Platform"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "extra": {
        "cosnics": {
            "name": "Admin",
            "context": "Chamilo\\Application\\Weblcms\\Admin\\Extension\\Platform",
            "type": "Chamilo\\Application\\Weblcms\\Admin\\Extension",
            "css": [
                {
                    "output": "cosnics.common.min.css",
                    "themes": [
                        {
                            "output": "cosnics.common.Aqua.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        },
                        {
                            "output": "cosnics.common.Ruby.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        }
                    ]
                }
            ],
            "install": {
                "default": 1,
                "core": 0
            },
            "dependencies": [
                {
                    "id": "Chamilo\\Application\\Weblcms",
                    "version": ">=1.0.0"
                }
            ]
        }
    }
}