<?xml version="1.0" encoding="UTF-8"?>
<object name="weblcms_admin">
    <properties>
        <property name="id"
                  type="integer"
                  notnull="1"
                  autoincrement="true"/>
        <property name="origin"
                  type="integer"
                  default="0"/>
        <property name="entity_type"
                  type="integer"
                  default="0"/>
        <property name="entity_id"
                  type="integer"
                  default="0"/>
        <property name="target_type"
                  type="integer"
                  default="0"/>
        <property name="target_id"
                  type="integer"
                  default="0"/>
        <property name="created"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="modified"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="idx_entity_type">
            <property name="entity_type"/>
        </index>
        <index name="idx_entity_id">
            <property name="entity_id"/>
        </index>
        <index name="unq_entity_id"
               type="unique">
            <property name="entity_type"/>
            <property name="entity_id"/>
            <property name="target_type"/>
            <property name="target_id"/>
        </index>
    </indexes>
</object>
