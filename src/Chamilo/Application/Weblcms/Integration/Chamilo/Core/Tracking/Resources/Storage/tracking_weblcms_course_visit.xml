<?xml version="1.0" encoding="UTF-8"?>
<object name="tracking_weblcms_course_visit">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="user_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="course_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="tool_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="category_id"
                  type="integer"
                  unsigned="1"/>
        <property name="publication_id"
                  type="integer"
                  unsigned="1"/>
        <property name="first_access_date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="last_access_date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="total_number_of_access"
                  type="integer"/>
        <property name="total_time"
                  type="integer"/>
    </properties>
    <indexes>
        <index name="unq_access"
               type="unique">
            <property name="user_id"/>
            <property name="course_id"/>
            <property name="tool_id"/>
            <property name="category_id"/>
            <property name="publication_id"/>
        </index>
    </indexes>
</object>