<?xml version="1.0" encoding="UTF-8"?>
<object name="tracking_weblcms_assessment_attempt">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="user_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="course_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="assessment_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="total_score"
                  type="integer"/>
        <property name="status"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="1"/>
        <property name="start_time"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="end_time"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="total_time"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="idx_assessment_id">
            <property name="assessment_id"/>
        </index>
        <index name="idx_user_id">
            <property name="user_id"/>
        </index>
        <index name="idx_course_id">
            <property name="course_id"/>
        </index>
    </indexes>
</object>
