<?xml version="1.0" encoding="UTF-8"?>
<object name="peer_assessment_feedback">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="attempt_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <!-- the user that receives the scores: -->
        <property name="user_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="attempt_status_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="feedback"
                  type="text"/>
    </properties>
    <index name="attempt_id">
        <indexproperty name="attempt_id"/>
    </index>
    <index name="user_id">
        <indexproperty name="user_id"/>
    </index>
</object>
