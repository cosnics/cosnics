<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service
                id="Chamilo\Application\Weblcms\Integration\Chamilo\Core\Repository\Publication\Service\PublicationAggregator"
                autowire="true">
            <tag name="chamilo.core.repository.publication.publication_aggregator"/>
        </service>

        <service
                id="chamilo.application.weblcms.integration.chamilo.core.repository.publication.service.publication_aggregator.assignment_publication_service"
                class="Chamilo\Application\Weblcms\Integration\Chamilo\Core\Repository\Publication\Service\AssignmentPublicationService">
            <argument type="service"
                      id="chamilo.application.weblcms.integration.chamilo.core.tracking.service.assignment_service"/>
            <argument type="service"
                      id="chamilo.core.repository.workspace.repository.content_object_repository"/>
            <argument
                    type="string">Chamilo\Application\Weblcms\Integration\Chamilo\Core\Tracking\Storage\DataClass\Assignment\EntryAttachment
            </argument>
        </service>

        <service
                id="chamilo.application.weblcms.integration.chamilo.core.repository.publication.service.publication_aggregator.learning_path_assignment_publication_service"
                class="Chamilo\Application\Weblcms\Integration\Chamilo\Core\Repository\Publication\Service\AssignmentPublicationService">
            <argument type="service"
                      id="chamilo.application.weblcms.integration.chamilo.core.tracking.service.learning_path_assignment_service"/>
            <argument type="service"
                      id="chamilo.core.repository.workspace.repository.content_object_repository"/>
            <argument
                    type="string">Chamilo\Application\Weblcms\Integration\Chamilo\Core\Tracking\Storage\DataClass\LearningPath\Assignment\EntryAttachment
            </argument>
        </service>
    </services>
</container>