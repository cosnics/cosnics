<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true"
                  autoconfigure="true"
                  public="true"/>

        <service id="Chamilo\Application\Weblcms\Tool\Implementation\Assignment\Service\AssignmentDataProvider">
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Integration\Chamilo\Core\Tracking\Service\AssignmentService"/>
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Bridge\Assignment\Service\Entity\EntityServiceManager"/>
            <argument type="service"
                      id="Chamilo\Core\Queue\Service\JobProducer"/>
        </service>

        <service id="Chamilo\Application\Weblcms\Tool\Implementation\Assignment\Service\EntriesDownloader\EntriesDownloaderFactory">
            <argument type="service"
                      id="Symfony\Component\Translation\Translator"/>
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Storage\Repository\PublicationRepository"/>
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Tool\Implementation\Assignment\Storage\Repository\PublicationRepository"/>
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Bridge\Assignment\Service\Entity\EntityServiceManager"/>
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Integration\Chamilo\Core\Tracking\Service\AssignmentService"/>
            <argument type="service"
                      id="Chamilo\Libraries\File\Compression\ArchiveCreator\ArchiveCreator"/>
        </service>

        <service id="Chamilo\Application\Weblcms\Tool\Implementation\Assignment\Service\AssignmentPublicationService">
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Tool\Implementation\Assignment\Storage\Repository\PublicationRepository"/>
            <argument type="service"
                      id="Chamilo\Application\Weblcms\Integration\Chamilo\Core\Tracking\Service\AssignmentService"/>
        </service>

    </services>
</container>