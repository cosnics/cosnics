                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="request_time"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="status"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="process_type"
                  type="integer"
                  unsigned="1"
                  default="0"/>
        <property name="percentage"
                  type="integer"
                  unsigned="1"
                  default="0"/>
