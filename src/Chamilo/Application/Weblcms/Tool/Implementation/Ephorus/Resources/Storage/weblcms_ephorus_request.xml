        <property name="course_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="content_object_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
        <property name="author_id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  default="0"/>
