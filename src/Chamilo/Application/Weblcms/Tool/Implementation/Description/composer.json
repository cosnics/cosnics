{
    "name": "cosnics/application-weblcms-tool-implementation-description",
    "version": "1.0.0",
    "description": "Application Weblcms Tool Implementation Description",
    "keywords": [
        "Application",
        "Weblcms",
        "Tool",
        "Implementation",
        "Description"
    ],
    "homepage": "http://www.cosnics.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Vanpoucke",
            "email": "sven.vanpoucke@hogent.be"
        },
        {
            "name": "Hans De Bisschop",
            "email": "hans.de.bisschop@ehb.be"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "extra": {
        "cosnics": {
            "name": "Description",
            "context": "Chamilo\\Application\\Weblcms\\Tool\\Implementation\\Description",
            "type": "Chamilo\\Application\\Weblcms\\Tool",
            "resources": [
                {
                    "output": "Resources/Css/cosnics.common.min.css",
                    "themes": [
                        {
                            "output": "Resources/Css/cosnics.common.Aqua.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        },
                        {
                            "output": "Resources/Css/cosnics.common.Ruby.min.css",
                            "input": [
                                "Resources/Css/Aqua/Stylesheet.min.css"
                            ]
                        }
                    ]
                }
            ],
            "install": {
                "default": 1,
                "core": 0
            },
            "dependencies": [
                {
                    "id": "Chamilo\\Application\\Weblcms",
                    "version": ">=1.0.0"
                }
            ],
            "extra": {
                "course-section": "tool"
            }
        }
    }
}