<?xml version="1.0" encoding="UTF-8" ?>
<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.access_token_repository"
                 class="Chamilo\Application\Weblcms\Tool\Implementation\CourseGroup\Extension\Office365\Storage\Repository\AccessTokenRepository">
            <argument type="service" id="chamilo.libraries.platform.configuration.local_setting" />
        </service>

        <service id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.office365_repository_factory"
                 class="Chamilo\Application\Weblcms\Tool\Implementation\CourseGroup\Extension\Office365\Storage\Repository\Office365RepositoryFactory">
            <argument type="service" id="chamilo.configuration.service.configuration_consulter" />
            <argument type="service" id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.access_token_repository" />
        </service>

        <service id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.office365_repository"
                 class="Chamilo\Application\Weblcms\Tool\Implementation\CourseGroup\Extension\Office365\Storage\Repository\Office365Repository"
                 factory-service="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.office365_repository_factory"
                 factory-method="buildOffice365Repository" />

        <service id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.service.office365_service"
                 class="Chamilo\Application\Weblcms\Tool\Implementation\CourseGroup\Extension\Office365\Service\Office365Service">
            <argument type="service" id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.office365_repository" />
            <argument type="service" id="chamilo.libraries.platform.configuration.local_setting" />
        </service>

        <service id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.course_group_office365_reference_repository"
                 class="Chamilo\Application\Weblcms\Tool\Implementation\CourseGroup\Extension\Office365\Storage\Repository\CourseGroupOffice365ReferenceRepository">
            <argument type="service" id="chamilo.libraries.storage.data_manager.doctrine.data_class_repository" />
        </service>

        <service id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.service.course_group_office365_reference_service"
                 class="Chamilo\Application\Weblcms\Tool\Implementation\CourseGroup\Extension\Office365\Service\CourseGroupOffice365ReferenceService" >
            <argument type="service" id="chamilo.application.weblcms.tool.implementation.course_group.extension.office365.storage.repository.course_group_office365_reference_repository" />
        </service>
    </services>
</container>