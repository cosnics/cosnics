<?xml version="1.0" encoding="UTF-8"?>
<object name="weblcms_course_type">
    <properties>
        <property name="id"
                  type="integer"
                  notnull="1"
                  autoincrement="true"/>
        <property name="display_order"
                  type="integer"
                  notnull="1"
                  default="1"/>
        <property name="active"
                  type="boolean"
                  default="1"/>
        <property name="title"
                  type="text"
                  notnull="1"
                  length="50"/>
        <property name="description"
                  type="text"/>
    </properties>
    <indexes>
        <index name="idx_display_order">
            <property name="display_order"/>
        </index>
    </indexes>
</object>
