<?xml version="1.0" encoding="UTF-8"?>
<object name="course_create_request">
    <properties>
        <property
            name="id"
            type="integer"
            notnull="1"
            autoincrement="true" />
        <property
            name="course_name"
            type="text"
            length="50"
            notnull="1" />
        <property
            name="course_type_id"
            type="integer"
            notnull="1" />
        <property
            name="user_id"
            type="integer"
            notnull="1" />
        <property
            name="subject"
            type="text"
            length="50" />
        <property
            name="motivation"
            type="text" />
        <property
            name="creation_date"
            type="integer"
            unsigned="1"
            notnull="1" />
        <property
            name="decision_date"
            type="integer"
            unsigned="1" />
        <property
            name="decision"
            type="boolean" />
    </properties>
    <index name="course_type_id">
        <indexproperty name="course_type_id" />
    </index>
    <index name="user_id">
        <indexproperty name="user_id" />
    </index>
</object>
