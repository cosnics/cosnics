<?xml version="1.0" encoding="UTF-8"?>
<object name="weblcms_course">
    <properties>
        <property name="id"
                  type="integer"
                  notnull="1"
                  autoincrement="true"/>
        <property name="course_type_id"
                  type="integer"
                  default="0"/>
        <property name="titular_id"
                  type="integer"
                  default="0"/>
        <property name="category_id"
                  type="integer"
                  default="0"/>
        <property name="last_visit"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="last_edit"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="creation_date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="expiration_date"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="title"
                  type="text"
                  length="250"
                  fixed="true"/>
        <property name="visual_code"
                  type="text"
                  length="40"
                  fixed="true"
                  notnull="1"/>
        <property name="system_code"
                  type="text"
                  length="40"
                  fixed="true"
                  notnull="1"/>
        <property name="language"
                  type="text"
                  length="40"
                  fixed="true"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="idx_course_type_id">
            <property name="course_type_id"/>
        </index>
        <index name="idx_titular_id">
            <property name="titular_id"/>
        </index>
        <index name="idx_category_id">
            <property name="category_id"/>
        </index>
    </indexes>
</object>