<?xml version="1.0" encoding="UTF-8"?>
<object name="weblcms_course_module">
    <properties>
        <property name="id"
                  type="integer"
                  unsigned="1"
                  notnull="1"
                  autoincrement="true"/>
        <property name="course_id"
                  type="integer"
                  notnull="1"/>
        <property name="name"
                  type="text"
                  length="40"
                  notnull="1"/>
        <property name="visible"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
        <property name="section"
                  type="integer"
                  notnull="1"/>
        <property name="sort"
                  type="integer"
                  unsigned="1"
                  notnull="1"/>
    </properties>
    <indexes>
        <index name="prm_id"
               type="unique">
            <property name="course_id"/>
            <property name="name"/>
        </index>
        <index name="idx_course_id">
            <property name="course_id"/>
        </index>
    </indexes>
</object>