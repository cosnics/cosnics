<?xml version="1.0" encoding="UTF-8"?>
<package context="Chamilo\Application\Plagiarism">
    <settings>
        <category name="general">
            <setting
                    name="maintenance_mode"
                    field="toggle"
                    default="1">
                <options type="static">
                    <option
                            name="maintenance_mode"
                            value="0" />
                </options>
            </setting>
        </category>
        <category name="turnitin">
            <setting name="turnitin_api_url" field="text" default="" />
            <setting name="turnitin_secret_key" field="text" default="" />
            <setting name="turnitin_webhook_id" field="text" locked="true" default="" />
            <setting name="turnitin_webhook_secret" field="text" locked="true" default="" />
            <setting name="turnitin_eula_accepted_date" field="text" locked="true" default="" user_setting="1" />
            <setting name="turnitin_eula_valid_from_date" field="text" locked="true" default="" />
            <setting name="turnitin_eula_valid_until_date" field="text" locked="true" default="" />
            <setting name="turnitin_eula_version" field="text" locked="true" default="" />
        </category>
        <category name="strike_plagiarism">
            <setting name="strike_plagiarism_api_url" field="text" default="" />
            <setting name="strike_plagiarism_secret_key" field="text" default="" />
            <setting name="strike_plagiarism_webhook_secret" field="text" locked="true" default="" />
        </category>
    </settings>
</package>
